					$Descr = $_.value3.Split(';')
                    If ($Items)
                    {
					    for ($i = 0; $i -lt $Items.Count; $i++)
					    {
                            $Item = [pscustomobject]@{
                                Value = $Items[$i]
                                Description = $Descr[$i]
                            }
						    $ListItems += $Item
					    }
                    }
					[pscustomobject]@{
						Name = $_.name
