	  <ScriptProperty>
		<Name>Parameters</Name>
		<GetScriptBlock>
			If ($this.Tasks.tasks.task)
			{
				$this.Tasks.tasks.task.parameters.param | %{
					switch ($_.Type)
					{
						0 {$Type = 'Text'}
						1 {$Type = 'List'}
						2 {$Type = 'Credentials'}
						3 {$Type = 'Multi-Select List'}
						4 {$Type = 'Multi-Line Text'}
						5 {$Type = 'Password'}
					}
					$ListItems = @()
					$Items = $_.value2.Split(';')
					$Descr = $_.value3.Split(';')
                    If ($Items)
                    {
					    for ($i = 0; $i -lt $Items.Count; $i++)
					    {
                            $Item = [pscustomobject]@{
                                Value = $Items[$i]
                                Description = $Descr[$i]
                            }
						    $ListItems += $Item
					    }
                    }
					[pscustomobject]@{
						Name = $_.name
						Description = $_.description
						Hint = $_.hint
						Type = $Type
						Default = $_.value1
						ListItems = $ListItems
					}
				}
			}
			else
			{
				"Use '-Full' parameter for details"
			}
		</GetScriptBlock>
	  </ScriptProperty>
	  <ScriptProperty>
		<Name>RequiredParameters</Name>
		<GetScriptBlock>
			If ($this.Tasks.tasks.task)
